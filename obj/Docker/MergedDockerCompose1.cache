services:
  auth-api:
    build:
      args:
        ASPNETCORE_ENVIRONMENT: Debug
      context: C:\Users\Elsav\Documents\Visual Studio 2019\Projects\MyFirstMicroService
      dockerfile: Auth-API/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Auth-API
      target: base
    container_name: Auth-API
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages;/root/.nuget/fallbackpackages2
    image: authapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages --additionalProbingPath
        /root/.nuget/fallbackpackages2  "/app/bin/Debug/netcoreapp3.1/Auth-API.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - /c/Users/Elsav/Documents/Visual Studio 2019/Projects/MyFirstMicroService/Auth-API:/app:rw
    - /c/Users/Elsav/vsdbg/vs2017u5:/remote_debugger:rw
    - /c/Users/Elsav/AppData/Roaming/ASP.NET/Https:/root/.aspnet/https:ro
    - /c/Users/Elsav/AppData/Roaming/Microsoft/UserSecrets:/root/.microsoft/usersecrets:ro
    - /c/Microsoft/Xamarin/NuGet:/root/.nuget/fallbackpackages:ro
    - /c/Program Files/dotnet/sdk/NuGetFallbackFolder:/root/.nuget/fallbackpackages2:ro
    - /c/Users/Elsav/.nuget/packages:/root/.nuget/packages:ro
    - /c/Users/Elsav/Documents/Visual Studio 2019/Projects/MyFirstMicroService:/src:rw
version: '3.4'