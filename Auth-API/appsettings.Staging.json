{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "AppSettings": {
    "JwtParameters": {
      "JwtIssuers": [ "http://localhost:8081", "https://localhost:8082" ],
      "JwtAudiences": [ "http://localhost:8083", "https://localhost:8084" ],
      "JwtKey": "YourJWTTokenEncryptionKey",
      "JwtExpiryInHours": "168"
    }
  },
  "ConnectionStrings": {
    "SQLServerConnection": "YourSQLConnectionString-Sta",
    "PostgreSQLConnection": "YourPostgreSQLConnectionString-Sta",
    "MySQLConnection": "YourMySQLConnectionString-Sta"
  },
  "SignUpKeys": {
    "Admin": "PasswordForAdminSignUp",
    "Owner": "PasswordForOwnerSignUp",
    "User": "PasswordForUserSignUp"
  }
}